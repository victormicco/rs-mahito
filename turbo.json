{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"preview": {
			"cache": false,
			"persistent": true
		},
		"type-check": {},
		"lint": {},
		"format": {},
		"check": {},
		"clean": {
			"cache": false
		}
	}
}
